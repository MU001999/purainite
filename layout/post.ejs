<article class="post">
    <div class="post-title">
        <h1 class="title"><%= page.title %></h2>
    </div>
    <div class="post-meta">
        <span class="post-time"><%- date(page.date, "YYYY 年 MM 月 DD日") %></span>
    </div>
    <div class="post-content">
        <%- page.content %>
    </div>
</article>
